    "jsdom": "~22.1.0",
    "nx": "21.3.8",
    "prettier": "^2.6.2",
    "sass": "^1.90.0",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.0.0",
    "vue-tsc": "^2.2.8"
